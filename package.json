  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "faunadb": "^2.6.1",
    "gatsby": "^2.12.0",
    "gatsby-image": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.1.6",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-sharp": "^2.2.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-odometerjs": "^2.1.1",
    "react-typist": "^2.0.5",
    "smooth-scroll": "^16.1.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "http-proxy-middleware": "^0.19.1",
    "netlify-lambda": "^1.4.13",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "checkForFaunaKey": "node ./scripts/check-for-fauna-key.js",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build": "npm-run-all --parallel build:**",
    "build:app": "gatsby build",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm-run-all --parallel checkForFaunaKey start:app start:server",
    "start:app": "gatsby develop",
    "start:server": "netlify-lambda serve functions -c ./webpack.config.js",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
